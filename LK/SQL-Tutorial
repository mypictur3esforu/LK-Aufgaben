select name, bip/einwohner from cia
where einwohner > 200e6

select name, bip/einwohner from cia
where einwohner > 200e6
order by bip/einwohner desc

select name from cia
where name like "%Vereinigte%"

select name, region from cia
order by region asc

select name, bip from cia
where bip is NULL


